          },
          "name": "meta.block.library_clause.vhdl",
          "patterns": [
            {
              "match": "(?i)([a-z][a-z0-9_]*)\\s*(,)?\\s*",
              "name": "meta.block.library_list_item.vhdl",
              "captures": {

{
  "comments": {
      "lineComment": "#",
      "blockComment": [ "/*", "*/" ]
  },
  "brackets": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"]
  ],
  "autoClosingPairs": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"],
      ["\"", "\""],
      ["'", "'"]
  ],
  "surroundingPairs": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"],
      ["\"", "\""],
      ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*#region\\b",
      "end": "^\\s*#endregion\\b"
    }
  }
}
